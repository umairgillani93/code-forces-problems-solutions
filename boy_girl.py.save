string = str(input('Enter name: ')).lower()

if string == '' or len(string) > 100:
    print('wrong strings')
else:
    char_count = {}
    
    for char in string:
        if char in char_count:
            char_count[char] += 1
        else:
            char_count[char] = 1
            
